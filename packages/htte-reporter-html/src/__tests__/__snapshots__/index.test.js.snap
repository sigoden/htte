            function switchFormat() {
                isJSON = !isJSON;
                $jsonDivs.forEach(function ($el) {
                    $el.classList.toggle('hidden');
                });
                $yamlDivs.forEach(function ($el) {
                    $el.classList.toggle('hidden');
                });
